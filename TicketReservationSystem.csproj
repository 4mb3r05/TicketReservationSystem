    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>aspnet-TicketReservationSystem-709fb81b-1224-4e31-b6a1-a9b61339964d</UserSecretsId>
